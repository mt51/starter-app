{
  "compilerOptions": {
    "lib": ["esnext"],
    "esModuleInterop": true,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "outDir": "dist",
    "module": "node16",
    "target": "es2022",
    "strict": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true
  },
  "include": ["**/*.ts", "*.ts"],
  "exclude": ["node_modules", "./prisma"]
}
